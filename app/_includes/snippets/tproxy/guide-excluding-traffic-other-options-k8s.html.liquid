<h3 class="no-anchor">Other configuration options</h3>

<table>
  <thead>
  <tr>
    <th colspan="2" style="text-align: left">
      <a href="/docs/{{ page.version }}/networking/transparent-proxy/configuration-on-kubernetes/#configuration-in-configmap">
        <strong>Configuration in ConfigMap</strong>
      </a>
    </th>
  </tr>
  <tr>
    <th style="text-align: left">Field</th>
    <th style="text-align: left">Environment Variable</th>
  </tr>
  </thead>
  <tbody>
  {% for field in include.data %}
    <tr>
      <td><code>{{ field.path }}</code></td>
      <td><code>{{ field.env }}</code></td>
    </tr>
  {% endfor %}
  </tbody>
</table>
<table>
  <thead>
  <tr>
    <th colspan="3" style="text-align: left">
      <a href="/docs/{{ page.version }}/networking/transparent-proxy/configuration-on-kubernetes/#control-plane-runtime-configuration">
        <strong>Control Plane Runtime Configuration</strong>
      </a>
    </th>
  </tr>
  <tr>
    <th style="text-align: left">Field</th>
    <th style="text-align: left">Environment Variable</th>
  </tr>
  </thead>
  <tbody>
  {% for field in include.data %}
    <tr>
      <td><code>{{ field.runtime.path }}</code></td>
      <td><code>{{ field.runtime.env }}</code></td>
    </tr>
  {% endfor %}
  </tbody>
</table>
